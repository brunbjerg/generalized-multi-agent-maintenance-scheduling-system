@article{meignanReviewTaxonomyInteractive2015,
	title = {A {{Review}} and {{Taxonomy}} of {{Interactive Optimization Methods
	         }} in {{Operations Research}}},
	author = {Meignan, David and Knust, Sigrid and Frayret, Jean-Marc and Pesant
	          , Gilles and Gaud, Nicolas},
	year = {2015},
