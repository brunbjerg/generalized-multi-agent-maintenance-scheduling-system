@inproceedings{meignanHeuristicApproachSchedule2014,
	title = {A Heuristic Approach to Schedule Reoptimization in the Context of
	         Interactive Optimization},
	booktitle = {{{GECCO}} 2014 - {{Proceedings}} of the 2014 {{Genetic}} and {{
