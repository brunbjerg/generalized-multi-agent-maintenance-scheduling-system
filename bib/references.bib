@article{perezhernandezMultiAgentLearningAsset2024,
	title = {Multi-{{Agent Learning}} of {{Asset Maintenance Plans}} through {{
	         Localised Subnetworks}}},
	author = {P{\'e}rez Hern{\'a}ndez, Marco and Puchkova, Alena and Parlikad,
