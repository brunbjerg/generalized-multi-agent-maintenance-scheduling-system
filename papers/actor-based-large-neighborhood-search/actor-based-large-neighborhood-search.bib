	pmcid = {PMC9890517},
	file = {PubMed Central Full Text PDF:/home/cbrje/Zotero/storage/E26QQHEP/Thomas and Weiss - 2021 - Maintenance Costs and Advanced Maintenance Techniq.pdf:application/pdf},
